{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Proxy List Leecher",
  "version": "0.0.1",
  "description": "Extract proxies easily from Proxy List websites and have them auto validated in the background.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "permissions": ["activeTab", "tabs", "https://*/*", "http://*/*", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
